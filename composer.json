        },
        "files": [
            "api/helpers.php"
        ]
    },
    "require": {
        "php": ">=8.3",
        "dibi/dibi": "^5.0",
        "dikki/config": "^2.0",
        "dikki/dotenv": "^2.0",
        "laminas/laminas-diactoros": "^3.5",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "league/container": "^4.2",
        "league/route": "^6.2",
        "nette/utils": "^4.0",
        "symfony/console": "^7.2",
        "symfony/var-dumper": "^7.2",
        "tracy/tracy": "^2.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.0"
